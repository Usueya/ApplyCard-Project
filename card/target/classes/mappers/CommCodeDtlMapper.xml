<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.risingsun.my.CommCodeDtlMapper">

	<select id="selectList" resultType="com.risingsun.mycard.dto.CommCodeDtl">
		SELECT * FROM COMM_CODE_DTL
	</select>
	
	<select id="selectOne" resultType="com.risingsun.mycard.dto.CommCodeDtl">
		SELECT * FROM COMM_CODE_DTL
		WHERE GROUP_CD = #{group_cd}
		AND CODE = #{code}
	</select>
	
</mapper>