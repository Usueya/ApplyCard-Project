<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- From snakeCase to CamelCase -->
 	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.risingsun.my.dto.ComCode" alias="ComCode"/>
		<typeAlias type="com.risingsun.my.dto.Board" alias="Board"/>
		<typeAlias type="com.risingsun.my.dto.UserInfo" alias="UserInfo"/>
	</typeAliases>
  
</configuration>



